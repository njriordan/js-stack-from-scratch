{
  "name": "js-stack-from-scratch",
  "version": "0.1.1",
  "description": "Experimenting with the JS stack",
  "main": "index.js",
  "repository": "https://github.com/njriordan/js-stack-from-scratch.git",
  "author": "Niall Riordan",
  "license": "MIT",
  "scripts": {
    "start": "yarn dev:start",
    "dev:start": "nodemon -e js,jsx --ignore lib --ignore dist --exec babel-node src/server",
    "dev:wds": "webpack-dev-server --progress",
    "prod:build": "rimraf lib dist && babel src -d lib --ignore .test.js && cross-env NODE_ENV=production webpack -p --progress",
    "prod:start": "heroku local",
    "heroku-postbuild": "yarn prod:build",
    "lint": "eslint src webpack.config.babel.js --ext .js,.jsx",
    "test": "yarn lint && flow && jest --coverage",
    "precommit": "yarn test",
    "prepush": "yarn test && yarn prod:build"
  },
  "browserslist": [
    "> 1%"
  ],
  "dependencies": {
    "add": "^2.0.6",
    "babel-polyfill": "^6.23.0",
    "bootstrap": "4.0.0-alpha.6",
    "common-tags": "^1.4.0",
    "compression": "^1.6.2",
    "express": "^4.15.3",
    "husky": "^0.13.4",
    "immutable": "4.0.0-rc.2",
    "isomorphic-fetch": "^2.2.1",
    "jquery": "^3.2.1",
    "react": "^15.5.4",
    "react-dom": "^15.5.4",
    "react-helmet": "^5.1.3",
    "react-hot-loader": "next",
    "react-jss": "5.3.0",
    "react-redux": "^5.0.5",
    "react-router": "^4.1.1",
    "react-router-dom": "^4.1.1",
    "redux": "^3.6.0",
    "redux-actions": "^2.0.3",
    "redux-thunk": "^2.2.0",
    "socket.io": "^2.0.3",
    "socket.io-client": "^2.0.3",
    "tether": "^1.4.0",
    "yarn": "^0.27.5"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-core": "^6.24.1",
    "babel-eslint": "^7.2.3",
    "babel-jest": "^20.0.3",
    "babel-loader": "^7.0.0",
    "babel-plugin-flow-react-proptypes": "^3.2.0",
    "babel-preset-env": "^1.5.1",
    "babel-preset-flow": "^6.23.0",
    "babel-preset-react": "^6.24.1",
    "coveralls": "^2.13.1",
    "cross-env": "^5.0.0",
    "eslint": "^3.19.0",
    "eslint-config-airbnb": "^15.0.1",
    "eslint-plugin-compat": "^1.0.3",
    "eslint-plugin-flowtype": "^2.34.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^5.0.1",
    "eslint-plugin-react": "^7.0.1",
    "fetch-mock": "^5.11.0",
    "flow-bin": "^0.47.0",
    "jest": "^20.0.4",
    "nodemon": "^1.11.0",
    "redux-mock-store": "^1.2.3",
    "rimraf": "^2.6.1",
    "webpack": "^2.6.1",
    "webpack-dev-server": "^2.4.5"
  },
  "engines": {
    "node": "7.x",
    "yarn": "0.20.3"
  }
}
